{
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "name": "fanout-common",
    "description": "Common code between the ecs-fanout and the lambda-fanout",
    "version": "3.0.5",
    "author": "Symphony",
    "license": "UNLICENSED",
    "main": "dist/index.js",
    "keywords": [],
    "homepage": "https://github.com/SymphonyOSF/symphony-datafeed-fanout-common",
    "scripts": {
        "build": "babel src -d dist --source-maps 'inline'",
        "rebuild": "npm run clean && npm run build",
        "clean": "rm -rf dist",
        "prepare": "npm run rebuild",
        "lint": "eslint --ext .js src",
        "test": "nyc mocha --recursive spec/unit",
        "security-check": "npm audit --registry=https://registry.npmjs.org --production"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run test && npm run security-check"
        }
    },
    "dependencies": {
        "lodash": "4.17.21",
        "symphony-datafeed-core": "5.0.5"
    },
    "devDependencies": {
        "@babel/cli": "7.14.5",
        "@babel/core": "7.14.6",
        "@babel/eslint-parser": "7.14.7",
        "@babel/node": "7.14.7",
        "@babel/preset-env": "7.14.7",
        "@babel/register": "7.14.5",
        "@istanbuljs/nyc-config-babel": "3.0.0",
        "babel-plugin-istanbul": "6.0.0",
        "babel-plugin-rewire": "1.2.0",
        "chai": "4.3.4",
        "chai-as-promised": "7.1.1",
        "eslint": "7.30.0",
        "eslint-config-airbnb-base": "14.2.1",
        "husky": "5.0.9",
        "mocha": "9.0.2",
        "nyc": "15.1.0",
        "sinon": "11.1.1",
        "sinon-chai": "3.7.0"
    },
    "nyc": {
        "all": false,
        "include": [
            "src/**/*.js"
        ],
        "exclude": [
            "**/*.spec.js"
        ]
    }
}
